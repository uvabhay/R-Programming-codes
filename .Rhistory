sum <- function(x, y) {
z <- x + y
return(z)
}
sum(2, 4)
# it returns all the formal parameters of a function
formals(sum)
fun <- function(x, y, z) {
print(paste(x, y, z))
}
# positional arguments
fun(2, 3, 4)
# keyword arguments (position of arguments does not matter)
fun(y = 4, z = 7, 5)
